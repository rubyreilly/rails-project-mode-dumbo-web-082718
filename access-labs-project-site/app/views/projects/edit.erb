<div class="title-header">Edit Project</div>
<div class="form-container">

<%= form_for @project, url: user_project_path(user_id: @user, id: @project) do |f| %>

<div><%= f.label :title, "Title"%></div>
<div><%= f.text_field :title %></div>

<div><%= f.label :mod_created, "Created in Mod" %></div>
<div><%= f.select :mod_created,  options_for_select([1, 2, 3, 4, 5], @project.mod_created) %></div>
<!-- <select name="project[mod_created]">
  <option value=1>1</option>
  <option value=2>2</option>
  <option value=3>3</option>
  <option value=4>4</option>
  <option value=5>5</option>
</select></div> -->
<div><%= f.label :github_link, "Github Link" %></div>
<div><%= f.text_field :github_link %></div>

<div><%= f.label :image_url, "Image URL" %></div>
<div><%= f.text_field :image_url %></div>

<div><%= f.submit%></div>
<% end %>
</div>
