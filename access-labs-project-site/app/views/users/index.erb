<div class="title-header">Students</div>
<div class="student-index-container">
<% @users.each do |user| %>
<div class="student-info-container mod<%=user.cohort.current_mod%>">
<div class="profile-thumbnail"><%= image_tag user.image_url, class: "profile-image-thumbnail" %></div>
<div><%= link_to user.full_name, user %></div>
<div><%= link_to user.cohort.name, cohorts_path(user.cohort) %></div>
</div>
<% end %>
</div>
